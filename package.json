{
  "name": "sails-postgresql",
  "description": "a postgreSQL adapter for Waterline and Sails.js",
  "version": "0.12.1",
  "author": "Cody Stoltman <particlebanana@gmail.com>",
  "url": "http://github.com/balderdashy/sails-postgresql",
  "keywords": [
    "postgresql",
    "orm",
    "waterline",
    "sails"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/balderdashy/sails-postgresql.git"
  },
  "dependencies": {
    "async": "1.5.2",
    "lodash": "3.10.1",
    "pg": "4.4.4",
    "waterline-cursor": "0.0.6",
    "waterline-errors": "0.10.1",
    "waterline-sequel": "0.6.2"
  },
  "devDependencies": {
    "captains-log": "0.11.11",
    "mocha": "2.4.5",
    "should": "8.2.1",
    "waterline-adapter-tests": "0.12.0"
  },
  "scripts": {
    "test": "make test",
    "docker": "docker-compose run adapter bash"
  },
  "main": "lib/adapter",
  "directories": {
    "lib": "./lib"
  },
  "license": "MIT",
  "bugs": "https://github.com/balderdashy/sails-postgresql/issues",
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.0",
    "interfaces": [
      "semantic",
      "queryable",
      "migratable",
      "associations",
      "sql"
    ],
    "features": [
      "crossAdapter",
      "unique",
      "autoIncrement",
      "schemas"
    ]
  }
}
